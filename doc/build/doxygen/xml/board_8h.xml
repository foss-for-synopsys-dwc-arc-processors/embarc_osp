      <memberdef kind="function" id="group__BOARD__COMMON_gadf80b95e6ceb5c1014e3e1a6f243fe8e_1gadf80b95e6ceb5c1014e3e1a6f243fe8e" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
