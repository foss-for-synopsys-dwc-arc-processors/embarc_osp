        <initializer><ref refid="group__DEVICE__HAL__IIC__CTRLCMD_gad396695c91a123a9bd9258a07e055051_1gad396695c91a123a9bd9258a07e055051" kindref="member">DEV_SET_IIC_SYSCMD</ref>(6)</initializer>
